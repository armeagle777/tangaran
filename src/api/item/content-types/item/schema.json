{
  "kind": "collectionType",
  "collectionName": "items",
  "info": {
    "singularName": "item",
    "pluralName": "items",
    "displayName": "item",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "region": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::region.region",
      "inversedBy": "items"
    },
    "number": {
      "type": "string",
      "required": true
    },
    "subNumber_1": {
      "type": "string"
    },
    "subNumber_2": {
      "type": "string"
    },
    "name": {
      "type": "string",
      "required": true
    },
    "period": {
      "type": "string",
      "required": true
    },
    "coordinates": {
      "type": "text",
      "required": true
    },
    "comment": {
      "type": "text"
    },
    "status": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "media": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "meaning": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::meaning.meaning",
      "inversedBy": "items"
    },
    "type": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::type.type",
      "inversedBy": "items"
    },
    "community": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::community.community",
      "inversedBy": "items"
    },
    "regionCommunity": {
      "type": "customField",
      "required": true,
      "customField": "plugin::region-community.region"
    }
  }
}
